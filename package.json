{
  "name": "@tim-code/rewrite-imports",
  "version": "0.1.0",
  "description": "",
  "main": "./index.cjs",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "type": "module",
  "scripts": {
    "test": "node --no-warnings --experimental-vm-modules node_modules/.bin/jest",
    "prepublishOnly": "babel index.js --out-file index.cjs"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@tim-code/autotest": "^1.0.1",
    "@tim-code/eslint-config": "^0.6.5",
    "jest": "^27.5.1"
  },
  "jest": {
    "transform": {}
  },
  "eslintConfig": {
    "extends": "@tim-code",
    "ignorePatterns": "index.cjs",
    "rules": {
      "no-restricted-syntax": "off"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 12
          }
        }
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
